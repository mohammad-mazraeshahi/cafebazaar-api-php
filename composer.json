{
  "name": "mohammad-mazraeshahi/cafebazaar-api",
  "description": "A PHP API wrapper for CafeBazaar Rest Api v2",
  "keywords": [
    "CafeBazaar",
    "API",
    "Bazaar",
    "Wrapper",
    "Cafebazaar Api",
    "In App Purchase",
    "iap"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Mazraeshahi",
      "email": "m.mazraeshahi@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "~5.3|~6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Mazraeshahi\\CafeBazaarApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mazraeshahi\\CafeBazaarApi\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
